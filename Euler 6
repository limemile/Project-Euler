<html>
<body>
	<script type = "text/javascript">
		function diff(max, power){
			sumSquares=0
			squareSum=0
				for( i = 1; i <= max; i++ ) {
					sumSquares += Math.pow(i, power);
					squareSum += i;
				}
				
				squareSum = Math.pow(squareSum, power);
				document.write(squareSum - sumSquares);
		}
		diff(100, 2);
	</script>
</body>
</html>
